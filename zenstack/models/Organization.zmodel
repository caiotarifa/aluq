import '../types/BaseModel'

import './Member'
import './Invitation'
import './BusinessUnit'

model Organization with BaseModel {
  name String
  slug String @unique
  logo String?

  defaultLocale String @default('pt-BR')
  defaultCurrencyCode String @default('BRL')

  metadata String?

  members Member[]
  invitations Invitation[]
  businessUnits BusinessUnit[]

  @@map('organization')
}
